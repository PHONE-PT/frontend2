<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 내부 채우기 색상: 원본 Vector의 #F7F5FF 색상 -->
    <solid android:color="#F7F5FF" />

    <!-- 둥근 모서리: 원본 Vector의 모서리 반경(약 4dp)을 반영하여 설정 -->
    <corners android:radius="8dp" />

    <!-- 테두리 선: 원본 Vector의 strokeWidth=2와 strokeColor=#BCB0F5 색상 -->
    <stroke
        android:width="2dp"
        android:color="#BCB0F5" />
</shape>